{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry"
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }], // es6 修饰器 插件
    "@babel/plugin-transform-runtime",
    "@babel/plugin-syntax-dynamic-import", // 支持webpack按需加载 import()方法 插件
    "@babel/plugin-transform-react-jsx"
  ]
}